<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- JAVA와 연결할 Mapper 파일 설정 -->
<mapper namespace="poly.persistance.mapper.ADMIN_Order_ItemMapper">
	<select id="getOrder_Item_List" parameterType="integer" resultType="ADMIN_Order_ItemDTO">
		SELECT 
			ITEM_SEQ,
			MENU_SEQ,
			USER_SEQ,
			ORD_AMNT,
			ORD_STATUS
		FROM ORDER_ITEM WHERE ITEM_SEQ=#{item_seq}
	</select>
</mapper>